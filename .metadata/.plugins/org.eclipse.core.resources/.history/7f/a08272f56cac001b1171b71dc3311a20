package com.java.ponteiros;
import java.util.Scanner;

public class Ponteiro {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);

		//New trata de reservar memória o suficiente para o objeto e criar automaticamente uma referência a ele. 
		Object A = new Object(); 
		A = 1;

		//Aqui o B aponta para o endereço de memória de A
		Object B = A;

		//Resultado impresso: valor qualquer
		System.out.printf("The B value is: "+B);	

		A = 2;
		B = A;
		System.out.println("New B value is: "+B);

	}
}
